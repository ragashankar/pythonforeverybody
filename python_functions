#Function - Call by reference

x = 7
print'Hello'

def print_lyrics(a):
    a=a-2
    print a
print 'Yo'
print_lyrics(x)
x = x+2

print x


-------------------------------------------------------------------------------------------------------------------------------------

#Function - Call by Value

x = 7
print'Hello'

def print_lyrics(x):
    x=x-2
    print x
print 'Yo'
print_lyrics(x)
x = x+2

print x
